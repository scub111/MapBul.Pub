{
  "name": "@mapbul-pub/server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build:init": "ncp tsconfig.build.json tsconfig.json && webpack",
    "build": "node scripts/buildServer",
    "build:tsc": "rimraf dist && tsc -p tsconfig.build.json",
    "debug": "ncp tsconfig.debug.json tsconfig.json && cross-env NODE_ENV=development ts-node-dev --project tsconfig.debug.json -r tsconfig-paths/register  --inspect=6590 --respawn src/main.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mapbul-pub/common": "^1.0.0",
    "@mapbul-pub/codegen": "^1.0.0",
    "@nestjs/common": "^6.9.0",
    "@nestjs/core": "^6.9.0",
    "@nestjs/platform-express": "^6.9.0",
    "app-root-path": "^3.0.0",
    "hbs": "^4.0.6",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.9.0",
    "@types/app-root-path": "^1.2.4",
    "@types/express": "4.17.2",
    "@types/jest": "24.0.21",
    "@types/mysql": "^2.15.7",
    "@types/node": "12.12.5",
    "@types/supertest": "2.0.8",
    "cross-env": "^6.0.3",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "8.4.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "tsc-watch": "4.0.0",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
