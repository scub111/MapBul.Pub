<configuration>
    <system.webServer>
        <handlers>
            <add name="iisnode" path="*.js" verb="*" modules="iisnode" />
        </handlers>
        <rewrite>
            <rules>
				<rule name="No redirect" stopProcessing="true">
					<match url="status.js" ignoreCase="true" />
					<action type="None" />
				</rule>
                <rule name="sendToNode">
                    <match url="(.*)" />
                    <action type="Rewrite" url="scripts/main.js" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>