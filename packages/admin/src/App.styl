.content
  min-width: 960px;
  height 100%;


.appContainerLogin {
  background: linear-gradient(to top, #dbf1ff, #ffffff);
}

:global {
  div.notification-warning {
    background: Yellow600;
    border-left: 8px solid #ce9c09a3;

    p.notification-message, p.notification-title {
      color: Basic1000;
      // font-family fontFamily;
    }

    p.notification-message {
      font-size fontSizeDefault;
    }

    p.notification-title {
      font-size fontSizeBold;
    }
  }

  .react-datepicker-popper {
    z-index: 16 !important;
  }
}
