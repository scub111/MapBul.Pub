{
   "name": "mapbul-pub",
   "version": "0.1.1",
   "description": "",
   "author": "",
   "license": "MIT",
   "scripts": {
      "server-build": "rimraf dist/server dist/tsconfig.server-build.tsbuildinfo && copyfiles src/server/.env dist/server -f && copyfiles src/server/views/* dist/server/views -f && copyfiles src/server/api.txt dist/server -f && tsc -p tsconfig.server-build.json --diagnostics | gnomon",
      "server-debug": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --inspect=6589 --respawn src/server/main.ts | gnomon",
      "format": "prettier --write \"src/**/*.ts\"",
      "start": "ts-node -r tsconfig-paths/register src/main.ts",
      "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
      "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect=6589 dist/main.js\"",
      "start:prod": "node dist/main.js",
      "lint": "tslint -p tsconfig.json -c tslint.json",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./__test__/jest-e2e.json",
      "codegen-build": "rimraf dist/codegen && tsc -p tsconfig.codegen-build.json --diagnostics | gnomon",
      "codegen-debug": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --inspect=6589 --respawn src/codegen/index.ts | gnomon",
      "scripts-build": "rimraf scripts && tsc -p tsconfig.scripts-build.json --diagnostics | gnomon",
      "scripts-debug": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --inspect=6589 --respawn src/scripts/buildServer.ts | gnomon"
   },
   "_moduleAliases": {
      "common": "dist/common/",
      "server": "dist/server/",
      "client": "dist/client/",
      "codegen": "dist/codegen/"
   },
   "dependencies": {
      "@nestjs/common": "^6.0.0",
      "@nestjs/core": "^6.0.0",
      "@nestjs/platform-express": "^6.0.0",
      "@types/app-root-path": "^1.2.4",
      "app-root-path": "^2.2.1",
      "dotenv": "^8.1.0",
      "hbs": "^4.0.4",
      "module-alias": "^2.2.1",
      "mysql": "^2.17.1",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^2.6.2",
      "rxjs": "^6.3.3"
   },
   "devDependencies": {
      "@nestjs/testing": "^6.0.0",
      "@types/express": "4.16.1",
      "@types/fs-extra": "^8.0.0",
      "@types/jest": "24.0.11",
      "@types/mkdirp": "^0.5.2",
      "@types/mysql": "^2.15.7",
      "@types/node": "11.13.4",
      "@types/supertest": "2.0.7",
      "artillery": "^1.6.0-28",
      "axios": "^0.19.0",
      "copyfiles": "^2.1.1",
      "cross-env": "^5.2.1",
      "es6-promise-pool": "^2.5.0",
      "fs-extra": "^8.1.0",
      "handlebars": "^4.2.0",
      "jest": "24.7.1",
      "mkdirp": "^0.5.1",
      "prettier": "1.17.0",
      "react-script": "^2.0.5",
      "supertest": "4.0.2",
      "ts-jest": "24.0.2",
      "ts-morph": "^3.1.3",
      "ts-node": "8.1.0",
      "ts-node-dev": "^1.0.0-pre.42",
      "tsc-watch": "2.2.1",
      "tsconfig-paths": "3.8.0",
      "tslint": "5.16.0",
      "typescript": "^3.4.3",
      "util": "^0.12.1"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".spec.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
